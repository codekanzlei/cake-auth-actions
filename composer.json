{
	"name": "codekanzlei/cake-auth-actions",
	"description": "Simple ACL alternative for CakePHP 3",
	"license": "MIT",
	"type": "cakephp-plugin",
	"extra": {
    	"installer-name": "AuthActions"
	},
	"require": {
		"cakephp/plugin-installer": "*"
	},
	"require-dev": {
		"cakephp/cakephp": "3.4.*",
        "phpunit/phpunit": "*"
	},
	"autoload": {
		"psr-4": {
		    "AuthActions\\": "src"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "AuthActions\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
