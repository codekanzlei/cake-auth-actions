{
	"name": "codekanzlei/cake-auth-actions",
	"description": "Simple ACL alternative for CakePHP 3",
	"license": "MIT",
	"type": "cakephp-plugin",
	"extra": {
    	"installer-name": "AuthActions"
	},
	"require": {
		"cakephp/plugin-installer": "*",
		"cakephp/cakephp": "~3.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.4",
		"cakephp/cakephp-codesniffer": "^3.0",
		"scherersoftware/coding-standard": "^3.0"
	},
	"autoload": {
		"psr-4": {
		"AuthActions\\": "src"
		}
	}
}
